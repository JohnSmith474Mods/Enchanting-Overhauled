{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/enchanting-overhauled",
        "sources": "https://github.com/JohnSmith474Mods/Enchanting-Overhauled",
        "issues": "https://github.com/JohnSmith474Mods/Enchanting-Overhauled/issues"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/textures/item/enchanted_tome.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "johnsmith.enchantingoverhauled.Fabric"
        ],
        "client": [
          "johnsmith.enchantingoverhauled.client.FabricClient"
        ],
        "modmenu": [
          "johnsmith.enchantingoverhauled.client.gui.ModMenuIntegration"
        ],
        "fabric-datagen": [
          "johnsmith.enchantingoverhauled.datagen.FabricLootTableInjectionGenerator"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=${java_version}"
    },
    "suggests": {
        "modmenu": "*"
    }
}
  